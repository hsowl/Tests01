package ch2;

public class VarEx3 {

	public static void main(String[] args) {
		String s = "Park" + "Va";
		String name = s + 8.0;
		System.out.println(s);
		System.out.println(name);
		System.out.println(7 + "");
		System.out.println("" + 7);
		System.out.println("" + 7 + 7);
		System.out.println(7 + 7 + "");
		System.out.println(name + 7 + 7);
		System.out.println(name + (7 + 7));
		
		System.out.println("aaaaaaa");
		
		int num = 15;
		System.out.printf("%o, %d, %x, ",num,num,num);
		
		double d = 10.52456;
		System.out.printf("%.2f",d);
		
		System.out.println("-------------");
		
				
		
		
		
	}

}
